<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<Scene onKeyPressed="#onKeyPressed" onKeyReleased="#onKeyReleased" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mariobrosfx.Game">
    <AnchorPane prefHeight="384.0" prefWidth="399.0">
      <children>
         <Canvas fx:id="canvas" disable="true" height="618.0" width="548.0" />
         <Label fx:id="lblGame" layoutX="200.0" layoutY="200.0" onMouseClicked="#lblMouseClick" text="Start" />
         <Label fx:id="lblCanGo" layoutX="198.0" layoutY="113.0" text="Label" textFill="WHITE" />
         <Label fx:id="lblOnPlatform" layoutX="198.0" layoutY="140.0" text="Label" textFill="WHITE" />
         <Label fx:id="lblplayer" layoutX="198.0" layoutY="167.0" text="Label" textFill="WHITE" />
      </children></AnchorPane>
</Scene>
